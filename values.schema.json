{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "affinity": {
            "type": "object"
        },
        "autoscaling": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "maxReplicas": {
                    "type": "integer"
                },
                "minReplicas": {
                    "type": "integer"
                },
                "targetCPUUtilizationPercentage": {
                    "type": "integer"
                },
                "targetMemoryUtilizationPercentage": {
                    "type": "integer"
                }
            }
        },
        "env": {
            "type": "array"
        },
        "envFrom": {
            "type": "array"
        },
        "extraContainers": {
            "type": "array"
        },
        "extraVolumeMounts": {
            "type": "array"
        },
        "extraVolumes": {
            "type": "array"
        },
        "global": {
            "type": "object",
            "properties": {
                "commonLabels": {
                    "type": "object"
                },
                "fullnameOverride": {
                    "type": "string"
                },
                "nameOverride": {
                    "type": "string"
                }
            }
        },
        "image": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "pullPolicy": {
                    "type": "string"
                },
                "pullSecrets": {
                    "type": "array"
                },
                "repository": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "initContainers": {
            "type": "array"
        },
        "lifecycle": {
            "type": "object"
        },
        "livenessProbe": {
            "type": "object"
        },
        "nodeSelector": {
            "type": "object"
        },
        "persistence": {
            "type": "object",
            "properties": {
                "accessModes": {
                    "type": "string"
                },
                "enabled": {
                    "type": "boolean"
                },
                "mountPath": {
                    "type": "string"
                },
                "selector": {
                    "type": "object"
                },
                "size": {
                    "type": "string"
                },
                "storageClassName": {
                    "type": "string"
                }
            }
        },
        "podAnnotations": {
            "type": "object"
        },
        "podLabels": {
            "type": "object"
        },
        "podPriorityClassName": {
            "type": "string"
        },
        "podSecurityContext": {
            "type": "object",
            "properties": {
                "fsGroup": {
                    "type": "integer"
                }
            }
        },
        "readinessProbe": {
            "type": "object"
        },
        "replicas": {
            "type": "integer"
        },
        "resources": {
            "type": "object",
            "properties": {
                "limits": {
                    "type": "object",
                    "properties": {
                        "cpu": {
                            "type": "string"
                        },
                        "memory": {
                            "type": "string"
                        }
                    }
                },
                "requests": {
                    "type": "object",
                    "properties": {
                        "cpu": {
                            "type": "string"
                        },
                        "memory": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "runner": {
            "type": "object",
            "properties": {
                "config": {
                    "type": "object",
                    "properties": {
                        "data": {
                            "type": "string"
                        },
                        "enabled": {
                            "type": "boolean"
                        }
                    }
                },
                "instanceURL": {
                    "type": "string"
                },
                "token": {
                    "type": "object",
                    "properties": {
                        "fromSecret": {
                            "type": "object",
                            "properties": {
                                "key": {
                                    "type": "string"
                                },
                                "name": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                },
                "updateStrategy": {
                    "type": "object",
                    "properties": {
                        "type": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "securityContext": {
            "type": "object",
            "properties": {
                "privileged": {
                    "type": "boolean"
                }
            }
        },
        "serviceAccount": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "create": {
                    "type": "boolean"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "terminationGracePeriodSeconds": {
            "type": "integer"
        },
        "tolerations": {
            "type": "array"
        },
        "topologySpreadConstraints": {
            "type": "array"
        }
    }
}
